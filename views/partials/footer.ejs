<script type="text/javascript">
    let button = document.getElementById("getDark");
    let navbarDark = document.getElementById("navbar");
    let isDark = false;
        button.addEventListener("click",function(){
            if(isDark){
            document.body.style.background = "#000900";
            button.setAttribute("class","btn btn-outline-light my-2 my-sm-0");
            button.innerHTML = 'Light mode <i class="fas fa-power-off"></i>';
            navbarDark.setAttribute("class","navbar fixed-top navbar-expand-md navbar-dark bg-dark")
            }
            else 
            {document.body.style.background = "grey";
            button.setAttribute("class","btn btn-outline-dark my-2 my-sm-0");
            button.innerHTML = 'Dark mode <i class="fas fa-power-off"></i>';
            navbarDark.setAttribute("class","navbar fixed-top navbar-expand-md navbar-light bg-light")
            }
            isDark = !isDark;
        });

</script>
</body>
</html>
